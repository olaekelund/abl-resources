[{
	"match": "\"\"[a-zA-Z0-9_\\.\\-]+\"\"",
	"name": "string.double.source.abl"
}, {
	"begin": "(\"(?!\"[a-zA-Z]+)|\"\"[a-zA-Z]+)",
	"patterns": [{
		"match": "(~(x\\h{2}|[0-2][0-7]{,2}|3[0-6][0-7]|37[0-7]?|[4-7][0-7]?|.)|\"\")",
		"name": "constant.character.escape.abl"
	}],
	"name": "string.double.complex.abl",
	"end": "\"(?!\")"
}, {
	"match": "''[a-zA-Z0-9_\\.\\-]+''",
	"name": "string.single.source.abl"
}, {
	"begin": "('(?!'[a-zA-Z]+)|''[a-zA-Z]+)",
	"patterns": [{
		"match": "(~(x\\h{2}|[0-2][0-7]{,2}|3[0-6][0-7]|37[0-7]?|[4-7][0-7]?|.)|'')",
		"name": "constant.character.escape.abl"
	}],
	"name": "string.single.complex.abl",
	"end": "'(?!')"
}]